<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<div id="chart5_div"></div>
<script type="text/javascript">


    google.charts.load('current', {
        packages: ['corechart', 'bar']
    });
    google.charts.setOnLoadCallback(LoadData);

    function LoadData() {
        $.ajax({

            url: '@Url.Action("Time3","Statistics")',
            dataType: "json",
            type: "GET",
            error: function(xhr, status, error) {
                var err = eval("(" + xhr.responseText + ")");
                toastr.error(err.message);
            },
            success: function(data) {
                PopulationChart5(data);
                return false;
            }
        });
        return false;
    }

    function PopulationChart5(data) {
        var dataArray = [
            ['Step', 'Greedy3']
        ];
        $.each(data, function(i, item) {
            dataArray.push([item.step, item.greedy3]);
        });
        var data = google.visualization.arrayToDataTable(dataArray);
        var options = {
            title: 'Ratio of number of jobs to time of execution',
            chartArea: {
                width: '50%'
            },
            colors: ['#ffab91'],
            hAxis: {
                title: 'Number of jobs',
                minValue: 0
            },
            vAxis: {
                title: 'Time of Execution (ms)',
                viewWindow: {min: 0}
            },
            curveType: 'function'
        };
        var chart = new google.visualization.LineChart(document.getElementById('chart5_div'));

        chart.draw(data, options);
        return false;
    }
</script>

